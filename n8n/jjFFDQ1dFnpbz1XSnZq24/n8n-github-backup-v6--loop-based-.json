    {
      "parameters": {
        "jsCode": "\nconst config = $('Config').first().json;\nconst workflows = $input.first().json.data || [];\n\nreturn {\n  json: {\n    workflows: workflows,\n    anzahl: workflows.length,\n    index: 0,\n    stats: { success: 0, skipped: 0, errors: 0 },\n    threemaSecret: config.secret,\n    threemaFrom: config.threemaFrom,\n    threemaTo: config.threemaTo\n  }\n};\n"
      },
      "id": "ff2ecf33-2269-4464-a16d-51df34933dba",
      "name": "Initialisierung",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        -3408,
        1304
      ]
    },
    {
      "parameters": {
        "jsCode": "\nconst data = $input.first().json;\nconst workflows = data.workflows;\nconst currentIndex = data.index;\n\nif (currentIndex >= workflows.length) {\n  return { json: { ...data, shouldContinue: false } };\n}\n\nconst wf = workflows[currentIndex];\nconst workflowId = wf.id;\nconst workflowName = wf.name || 'Unnamed';\nconst cleanName = workflowName.replace(/[^a-zA-Z0-9-_]/g, '-').toLowerCase();\n\n// Base64 encode (Hostinger compatible)\nconst content = btoa(unescape(encodeURIComponent(JSON.stringify(wf, null, 2))));\n\nreturn {\n  json: {\n    ...data,\n    currentWorkflow: {\n      id: workflowId,\n      name: workflowName,\n      filepath: `n8n/${workflowId}/${cleanName}.json`,\n      content: content,\n      message: `ü§ñ Auto-Backup: ${workflowName}`\n    },\n    shouldContinue: true\n  }\n};\n"
      },
      "id": "bdc5a565-d96f-420e-962e-8e513f5c7b5f",
      "name": "Extrahiere Workflow",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        -3184,
        1304
      ]
    },
    {
      "parameters": {
        "resource": "file",
        "operation": "get",
        "owner": {
          "__rl": true,
          "value": "peerendees",
          "mode": "name"
        },
        "repository": {
          "__rl": true,
          "value": "n8n-workflows",
          "mode": "name"
        },
        "filePath": "={{ $json.currentWorkflow.filepath }}",
        "asBinaryProperty": false,
        "additionalParameters": {}
      },
      "id": "47cfb3ba-5e1e-4a04-a9a8-a123f2ae5d61",
      "name": "Check GitHub File",
      "type": "n8n-nodes-base.github",
      "typeVersion": 1.1,
      "position": [
        -2960,
        1232
      ],
      "webhookId": "9a733153-d97d-41a3-8425-6214ac2c7b47",
      "credentials": {
        "githubApi": {
          "id": "uE78TkEY6lWLV4oP",
          "name": "API GitHub"
        }
      },
      "onError": "continueRegularOutput"
    },
    {
      "parameters": {
        "jsCode": "\nconst data = $input.first().json;\nconst github = $json;\nconst local = data.currentWorkflow;\n\nlet action = 'skip';\nlet sha = None;\n\n// Check if file exists in GitHub\nif (github.error || github.message === 'Not Found') {\n  action = 'create';\n} else {\n  sha = github.sha;\n  \n  // Compare content\n  const remoteContent = (github.content || '').replace(/\\s/g, '');\n  const localContent = (local.content || '').replace(/\\s/g, '');\n  \n  if (remoteContent !== localContent) {\n    action = 'update';\n  }\n}\n\nreturn {\n  json: {\n    ...data,\n    currentWorkflow: {\n      ...local,\n      action: action,\n      sha: sha\n    }\n  }\n};\n"
      },
      "id": "73d2192d-d76a-49b3-bfd4-0daa40e2be00",
      "name": "Determine Action",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
