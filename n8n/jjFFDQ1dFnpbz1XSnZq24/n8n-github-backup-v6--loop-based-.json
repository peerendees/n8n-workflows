    {
      "parameters": {
        "jsCode": "\nconst data = $input.first().json;\nconst action = data.currentWorkflow.action;\n\n// Update stats basierend auf action\nconst newStats = { ...data.stats };\n\nif (action === 'skip') {\n  newStats.skipped += 1;\n} else if ($json.commit && $json.commit.sha) {\n  newStats.success += 1;\n} else {\n  newStats.errors += 1;\n}\n\nreturn {\n  json: {\n    ...data,\n    stats: newStats,\n    index: data.index + 1\n  }\n};\n"
      },
      "id": "d3e5de36-0318-4fb1-859d-e75de5ac25b1",
